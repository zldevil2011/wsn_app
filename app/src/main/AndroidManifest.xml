<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ll.wsn" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/wsn"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DataInfo"
            android:label="@string/title_activity_data_info" >
        </activity>
        <activity android:name="org.achartengine.GraphicalActivity" />
        <activity
            android:name="com.ll.chart.PieChart"
            android:label="@string/title_activity_pie_chart" >
        </activity>
        <activity
            android:name=".Temperature"
            android:label="@string/title_activity_temperature" >
        </activity>
        <activity
            android:name="com.ll.settings.Settings"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="com.ll.settings.About_us"
            android:label="@string/title_activity_about_us" >
        </activity>
        <activity
            android:name="com.ll.user.User"
            android:label="@string/title_activity_user" >
        </activity>
        <activity
            android:name="com.ll.user.PlaceChoose"
            android:label="@string/title_activity_place_choose" >
        </activity>
        <activity
            android:name="com.ll.user.EditInfo"
            android:label="@string/title_activity_edit_info" >
        </activity>
    </application>

</manifest>
